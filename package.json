{
  "name": "pantunflas-app",
  "version": "1.0.0",
  "private": true,
  "author": "Alejandro Montes de Oca, Axel Mendez, Erick Rodriguez",
  "description": "CRUD BÃ¡sico con Java y Js",
  "homepage": "./",
  "main": "public/electron.js",
  "dependencies": {
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^11.2.7",
    "@testing-library/user-event": "^12.8.3",
    "axios": "^0.21.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-scripts": "4.0.3",
    "reactstrap": "^8.9.0",
    "electron-is-dev": "^2.0.0",
    "web-vitals": "^1.1.2"
  },
  "devDependencies": {
    "concurrently": "^6.0.0",
    "electron-builder": "^22.10.5",
    "electron": "^13.1.4",
    "wait-on": "^5.3.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "electron:dev": "concurrently \"BROWSER=none npm run start\" \"wait-on http://localhost:3000 && electron .\"",
    "electron-build:windows": "electron-builder -c.extraMetadata.main=build/electron.js --windows",
    "electron-build:mac": "electron-builder -c.extraMetadata.main=build/electron.js --mac",
    "preelectron-pack": "npm run build"
  },
  "build": {
    "appId": "com.utcPOO2.utc",
    "files": [
      "build/**/*",
      "node_modules/**/*"
    ],
    "directories": {
      "buildResources": "assets"
    }
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}